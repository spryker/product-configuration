{
  "name": "spryker/product-configuration",
  "description": "ProductConfiguration module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/kernel": "^3.30.0",
    "spryker/product": "*",
    "spryker/propel-orm": "*"
  },
  "require-dev": {
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\ProductConfiguration\\Helper\\": "tests/SprykerTest/Shared/ProductConfiguration/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
